{% extends 'base.html.twig' %}

{% block stylesheets %}
    <link rel="stylesheet" href="/assets/css/product-card.css">
    <link rel="stylesheet" href="/assets/css/home-slider.css">
    <link rel="stylesheet" href="/assets/css/brand-slider.css">
{% endblock %}

{% block javascripts %}
    <script src="/assets/js/brand-slider.js"></script>
{% endblock %}

{% block body %}
    <!-- Start: Accueil -->
    <section>
        <div class="d-flex flex-column align-items-center flex-lg-row" style="height: 75svh;">
            <div class="h-50 home-slider-left" style="background: url('assets/img/slider_background.png'); width: 100%;">
                <div style="width: 50%;">
                    <h1 style="font-size: 7rem;color: var(--bs-primary);font-weight: bold;">Black Friday</h1>
                    <p style="color: var(--bs-secondary);font-size: 1.2rem;">Promos sur une sélection d'articles sur Génération Shoes !&nbsp;Des chaussures respectueuses des pieds des enfants à petit prix !</p>
                </div>
            </div>
            <div class="h-50 home-slider-right" style="width: 100%;">
                <picture><img class="h-100 w-100" src="assets/img/promo.png" width="" height=""></picture>
            </div>
        </div>
    </section>
    <!-- End: Accueil -->
    <div class="marquee container-fluid w-100">
        <ul class="marquee-content">
            <li><i class="fab fa-github"></i></li>
            <li><i class="fab fa-codepen"></i></li>
            <li><i class="fab fa-free-code-camp"></i></li>
            <li><i class="fab fa-dev"></i></li>
            <li><i class="fab fa-react"></i></li>
            <li><i class="fab fa-vuejs"></i></li>
            <li><i class="fab fa-angular"></i></li>
            <li><i class="fab fa-node"></i></li>
            <li><i class="fab fa-wordpress"></i></li>
            <li><i class="fab fa-aws"></i></li>
            <li><i class="fab fa-docker"></i></li>
            <li><i class="fab fa-android"></i></li>
        </ul>
    </div>
    <!-- Start: Produits Phares -->
    <section>
        <h1 class="container">Produits phares</h1>
        <!-- Start: products-grid -->
        <div class="bd-grid">
            {% for product in latestProducts %}
            <article class="card">
                {% if(pictures[product.id]) %}<div class="card__img"><img src="/assets/img/original/{{ pictures[product.id].original }}" alt=""></div>{% endif %}
